  <xacro:gazebo_link name="${robot_name}left_ring_distal"    gravityOff="false" static="false" collide="false"/>

  <xacro:gazebo_link name="${robot_name}left_little_proximal"  gravityOff="false" static="false" collide="false"/>
  <xacro:gazebo_link name="${robot_name}left_little_medial"    gravityOff="false" static="false" collide="false"/>
  <xacro:gazebo_link name="${robot_name}left_little_distal"    gravityOff="false" static="false" collide="false"/>


  <xacro:gazebo_link name="${robot_name}right_shoulder_roll"  gravityOff="false" static="false" collide="false"/>
