<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:macro name="gazebo_link" params="name gravityOff static collide">
     <gazebo reference="${name}">
        <selfCollide>${collide}</selfCollide>
        <static>${static}</static>
        <turnGravityOff>${gravityOff}</turnGravityOff>
     </gazebo>
  </xacro:macro>

  <xacro:macro name="gazebo_joint" params="name">
     <gazebo reference="${name}">
        <stopKd value="1.0" />
        <stopKp value="1000000.0" />
     </gazebo>
  </xacro:macro>

  <xacro:gazebo_link name="${robot_name}waist_center" gravityOff="false" static="false" collide="false"/>
  <xacro:gazebo_joint name="${robot_name}waist/joint0"/>

  <xacro:gazebo_link name="${robot_name}left_shoulder_roll"  gravityOff="false" static="false" collide="false"/>
  <xacro:gazebo_link name="${robot_name}left_shoulder_pitch" gravityOff="false" static="false" collide="false"/>
  <xacro:gazebo_link name="${robot_name}left_upper_arm"      gravityOff="false" static="false" collide="false"/>
  <xacro:gazebo_link name="${robot_name}left_elbow"          gravityOff="false" static="false" collide="false"/>
  <xacro:gazebo_link name="${robot_name}left_lower_arm"      gravityOff="false" static="false" collide="false"/>
  <xacro:gazebo_link name="${robot_name}left_wrist_pitch"    gravityOff="false" static="false" collide="false"/>
  <xacro:gazebo_link name="${robot_name}left_wrist_yaw"      gravityOff="false" static="false" collide="false"/>
  <xacro:gazebo_link name="${robot_name}left_palm"           gravityOff="false" static="false" collide="false"/>
  
  <xacro:gazebo_joint name="${robot_name}left_arm/joint0"/>
  <xacro:gazebo_joint name="${robot_name}left_arm/joint1"/>
  <xacro:gazebo_joint name="${robot_name}left_arm/joint2"/>
  <xacro:gazebo_joint name="${robot_name}left_arm/joint3"/>
  <xacro:gazebo_joint name="${robot_name}left_arm/joint4"/>
  <xacro:gazebo_joint name="${robot_name}left_arm/joint5"/>
  <xacro:gazebo_joint name="${robot_name}left_arm/joint6"/>

  <xacro:gazebo_link name="${robot_name}left_thumb_base"          gravityOff="false" static="false" collide="false"/>
  <xacro:gazebo_link name="${robot_name}left_thumb_medial_prime"  gravityOff="false" static="false" collide="false"/>
  <xacro:gazebo_link name="${robot_name}left_thumb_medial"        gravityOff="false" static="false" collide="false"/>
  <xacro:gazebo_link name="${robot_name}left_thumb_distal"        gravityOff="false" static="false" collide="false"/>

  <xacro:gazebo_link name="${robot_name}left_index_base"      gravityOff="false" static="false" collide="false"/>
  <xacro:gazebo_link name="${robot_name}left_index_proximal"  gravityOff="false" static="false" collide="false"/>
  <xacro:gazebo_link name="${robot_name}left_index_medial"    gravityOff="false" static="false" collide="false"/>
  <xacro:gazebo_link name="${robot_name}left_index_distal"    gravityOff="false" static="false" collide="false"/>

  <xacro:gazebo_link name="${robot_name}left_middle_base"      gravityOff="false" static="false" collide="false"/>
  <xacro:gazebo_link name="${robot_name}left_middle_proximal"  gravityOff="false" static="false" collide="false"/>
