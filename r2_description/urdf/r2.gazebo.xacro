  <xacro:gazebo_link name="${robot_name}right_middle_medial"    gravityOff="false" static="false" collide="false"/>
  <xacro:gazebo_link name="${robot_name}right_middle_distal"    gravityOff="false" static="false" collide="false"/>

  <xacro:gazebo_link name="${robot_name}right_ring_proximal"  gravityOff="false" static="false" collide="false"/>
  <xacro:gazebo_link name="${robot_name}right_ring_medial"    gravityOff="false" static="false" collide="false"/>
  <xacro:gazebo_link name="${robot_name}right_ring_distal"    gravityOff="false" static="false" collide="false"/>

  <xacro:gazebo_link name="${robot_name}right_little_proximal"  gravityOff="false" static="false" collide="false"/>
  <xacro:gazebo_link name="${robot_name}right_little_medial"    gravityOff="false" static="false" collide="false"/>
  <xacro:gazebo_link name="${robot_name}right_little_distal"    gravityOff="false" static="false" collide="false"/>


  <xacro:gazebo_link name="${robot_name}neck_base"         gravityOff="false" static="false" collide="false"/>
  <xacro:gazebo_joint name="${robot_name}neck/joint0"/>
  <xacro:gazebo_link name="${robot_name}neck_lower_pitch"  gravityOff="false" static="false" collide="false"/>
  <xacro:gazebo_joint name="${robot_name}neck/joint1"/>
  <xacro:gazebo_link name="${robot_name}neck_roll"         gravityOff="false" static="false" collide="false"/>
  <xacro:gazebo_joint name="${robot_name}neck/joint2"/>

</robot>
