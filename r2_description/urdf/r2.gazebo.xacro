  <xacro:gazebo_joint name="${robot_name}right_arm/joint0"/>
  <xacro:gazebo_joint name="${robot_name}right_arm/joint1"/>
  <xacro:gazebo_joint name="${robot_name}right_arm/joint2"/>
  <xacro:gazebo_joint name="${robot_name}right_arm/joint3"/>
  <xacro:gazebo_joint name="${robot_name}right_arm/joint4"/>
  <xacro:gazebo_joint name="${robot_name}right_arm/joint5"/>
  <xacro:gazebo_joint name="${robot_name}right_arm/joint6"/>

  <xacro:gazebo_link name="${robot_name}right_thumb_base"          gravityOff="false" static="false" collide="false"/>
  <xacro:gazebo_link name="${robot_name}right_thumb_medial_prime"  gravityOff="false" static="false" collide="false"/>
  <xacro:gazebo_link name="${robot_name}right_thumb_medial"        gravityOff="false" static="false" collide="false"/>
  <xacro:gazebo_link name="${robot_name}right_thumb_distal"        gravityOff="false" static="false" collide="false"/>

  <xacro:gazebo_link name="${robot_name}right_index_base"      gravityOff="false" static="false" collide="false"/>
  <xacro:gazebo_link name="${robot_name}right_index_proximal"  gravityOff="false" static="false" collide="false"/>
  <xacro:gazebo_link name="${robot_name}right_index_medial"    gravityOff="false" static="false" collide="false"/>
